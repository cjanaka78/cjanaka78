        <Label Content="Price:" Height="23" HorizontalAlignment="Left" Margin="0,21,0,0" Name="LabelPrice" VerticalAlignment="Top" Width="108" />
        <Label Content="Amt Tendered:" Height="28" HorizontalAlignment="Left" Margin="0,55,0,0" Name="labelAmtTendered" VerticalAlignment="Top" Width="108" />

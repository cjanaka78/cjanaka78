        <Label Content="Nickles:" Height="28" HorizontalAlignment="Left" Margin="0,191,0,0" Name="labelNickles" VerticalAlignment="Top" Width="108" />
        <Label Content="Pennies:" Height="28" HorizontalAlignment="Left" Margin="0,225,0,0" Name="labelPennies" VerticalAlignment="Top" Width="108" />
        <Button Content="Compute Change" Height="23" HorizontalAlignment="Left" Margin="281,21,0,0" Name="buttonComputeChange" VerticalAlignment="Top" Width="112" Click="buttonComputeChange_Click" />
    </Grid>
</Window>
